        <TextBox x:Name="FibonacciText" HorizontalAlignment="Left" Height="397" Margin="10,43,0,0" TextWrapping="Wrap" Text="" IsEnabled="False" VerticalAlignment="Top" Width="575" Grid.ColumnSpan="3"/>
