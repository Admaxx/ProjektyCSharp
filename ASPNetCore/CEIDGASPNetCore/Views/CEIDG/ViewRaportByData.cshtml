@model IEnumerable<CEIDGASPNetCore.DbModel.Gusvalue>

@{
    ViewData["Title"] = "ViewRaportByData";
}

@using (Html.BeginForm("ViewRaportByData","CEIDG",FormMethod.Post))
{
    <p>
        <input type="date" name="RaportDate" />
    </p>
    <input type="submit", value="Wybierz">
}

@foreach(var RaportData in Model)
{
    <dt class="col-sm-2">
        @Html.Label("Zawartość raportu")
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(item => RaportData.Xmlvalues)
    </dd>
    <dt class="col-sm-2">
        @Html.Label("Data raportu")
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(item => RaportData.ImportDate)
    </dd>
    <dt class="col-sm-2">
        @Html.Label("Typ raportu")
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(item => RaportData.RaportType)
    </dd>
    <br/>

}
<div>
    <a asp-action="Index">Wróć do menu</a>
</div>